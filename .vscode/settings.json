{
  "editor.formatOnSave": true,
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.eol": "\n",
  "colorWheel.settings.workspaceColor": "hsl(234,75%,43%)",
}
