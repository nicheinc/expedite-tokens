{
  "name": "@nicheinc/expedite-tokens",
  "version": "2.1.0",
  "description": "Design Tokens for the Expedite Design System",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "niche",
    "design-tokens",
    "expedite"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && node ./scripts/generateLessTokens.mjs",
